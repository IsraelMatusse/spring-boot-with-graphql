schema{
    query: getDepartamento
    mutation: addDepartamento
}

type getDepartamento{
    getAllDepartamento : [Departamento]
}
type addDepartamento{
    createDepartamento(input: DepartamentoInput):Departamento
}
input DepartamentoInput{
    codigo: String
    designacao: String
    objectivo: String
    endereco:String
}

type Departamento{
    id: String
    codigo: String
    designacao: String
    objectivo: String
    endereco:String

}